page{
  background: #eee;
  padding-bottom: 13%;
}
button::after{
  border: none;
}
/* 页头 */
.top{
  width: 100%;
  background: #fff;
}
.top image{
  width: 100%;
  height: 350rpx;
}
/* 明星客户 */
.mxky{
  width: 100%;
  padding: 20rpx;
  background: #fff;
  box-sizing: border-box;
}
.mxky-xq,.mx-xm{
  width: 100%;
  font-family: "微软雅黑";
  font-size: 28rpx;
  color: #242424;
  margin-top: 20rpx;
}
.mx-xm{
  font-size: 30rpx;
}
.mx-jz{
  width: 100%;
  font-family: "微软雅黑";
  font-size: 25rpx;
  color: #E49015;
  margin: 20rpx 0;
}
.mx-xq{
  width: 100%;
  background: #FFF5E5;
}
.mx-xq image{
  width: 100%;
  height: 450rpx;
}
.mx-lx{
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  border-bottom: 1px solid #EFE1CA;
}
.mx-lx view{
  width: 100%;
  font-family: "微软雅黑";
  font-size: 28rpx;
  color: #000;
}
.mx-btn{
  width: 100%;
  padding: 30rpx;
  box-sizing: border-box;
}
.mx-btn button{
  width: 500rpx;
  margin: 0 auto;
  border: 1px solid #E49015;
  background: transparent;
  font-family: "微软雅黑";
  font-size: 28rpx;
  color: #E49015;
}
/* 立即咨询明星同款月子套餐 */
.tongkuan{
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  background: #fff;
}
.tongkuan button{
  width: 100%;
  background: #E49015;
  font-family: "微软雅黑";
  font-size: 28rpx;
  color: #fff;
}
/* 猜你喜欢 */
.love{
  width: 100%;
  background: #fff;
  padding: 20rpx;
  box-sizing: border-box;
  margin-top: 20rpx;
}
.fuwu-title{
  width: 100%;
  text-align: center;
  font-family: "微软雅黑";
  font-size: 30rpx;
  color: #000;
  font-weight: bold;
}
.love-list{
  width: 100%;
  padding: 20rpx 0;
  box-sizing: border-box;
  overflow: hidden;
  border-bottom: 1px solid #eee;
}
.love-list:nth-child(2){
  border-top: 1px solid #eee;
  margin-top: 20rpx;
}
.love-list image{
  width: 120rpx;
  height: 120rpx;
  float: left;
}
.love-txt{
  width: 590rpx;
  float: left;
  padding: 5rpx 0 5rpx 20rpx;
  box-sizing: border-box;
}
.love-txt view{
  width: 100%;
  font-family: "微软雅黑";
  font-size: 28rpx;
  color: #000;
}
.love-txt view.love-txt1{
  height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.love-txt view.love-txt2{
  height: 70rpx;
  font-size: 26rpx;
  color: #A4A4A4;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin-top: 2rpx;
}
/* 入住名额 */
.qiang{
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  background: #fff;
  margin-top: 20rpx;
}
.qiang-form{
  width: 100%;
  margin-top: 20rpx;
  box-sizing: border-box;
}
.qiang-form view{
  width: 100%;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.qiang-form view input{
  width: 100%;
  height: 80rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  font-family: "微软雅黑";
  color: #000;
  font-size: 26rpx;
  border: 1px solid #eee;
}
.qiang-form view radio-group label{
  font-family: "微软雅黑";
  color: #000;
  font-size: 26rpx;
}
.qiang-form view radio-group label:first-child{
  float: left;
}
.qiang-form view radio-group label:last-child{
  float: right;
}
.qiang-form form button{
  width: 100%;
  border-radius: 0;
  background: #F79200;
  font-family: "微软雅黑";
  font-size: 30rpx;
  color: #fff;
}
button::after{
  border: none;
}
.qiang-di{
  width: 100%;
  margin-top: 20rpx;
  text-align: center;
  font-family: "微软雅黑";
  font-size: 28rpx;
  color: #606060;
}
.qiang-di text{
  color: #EDA983;
}
/* 版权 */
.banquan{
  width: 100%;
  box-sizing: border-box;
  text-align: center;
  padding: 20rpx 0;
}
.banquan image{
  width: 55rpx;
  height: 55rpx;
  margin-right: 10rpx;
}
.banquan-conp{
  display: inline-block;
  height: 60rpx;
}
.banquan-conp text{
  display: block;
  font-family: "微软雅黑";
  font-size: 22rpx;
  color: #8A8A8A;
}
/* 底部导航 */
.nav{
  width: 100%;
  height: 95rpx;
  box-sizing: border-box;
  position: fixed;
  bottom: 0;left: 0;
}
.nav button{
  display: inline-block;
  width: 33.33%;
  height: 95rpx;
  line-height: 67rpx;
  text-align: center;
  padding: 20rpx;
  box-sizing: border-box;
  border-radius: 0;
}
.nav button image{
  width: 45rpx;
  height: 45rpx;
  margin-right: 10rpx;
}
.nav button:nth-child(1){
  background: #C9355B;
}
.nav button:nth-child(2){
  background: #E49015;
}
.nav button:nth-child(3){
  background: #59325B;
}
.nav button text{
  font-family: "微软雅黑";
  font-size: 30rpx;
  color: #fff;
  font-weight: bold;
  position: relative;
  top: -11rpx;
}
/* 隐藏导航 */
.hn{
  width: 500rpx;
  height: 520rpx;
  border-radius: 20rpx;
  box-sizing: border-box;
  background: rgba(0, 0, 0, 0.6);
  position: fixed;
  bottom: 110rpx;
  right: 20rpx;
}
.hn-fenlei{
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  padding-bottom: 30rpx;
}
.hn-fenlei-list{
  float: left;
  width: 33.33%;
  text-align: center;
  box-sizing: border-box;
  margin-top: 30rpx;
}
.hn-fenlei-list image{
  width: 85rpx;
  height: 85rpx;
  border-radius: 50%;
}
.hn-fenlei-list view{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-family: "微软雅黑";
  color: #fff;
  font-size: 28rpx;
}
view.show{
  display: block;
}
view.hide{
  display: none;
}