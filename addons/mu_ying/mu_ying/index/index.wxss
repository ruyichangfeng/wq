page{
  background: #eee;
  padding-bottom: 13%;
}
/* 轮播图 */
.lunbo{
  width: 100%;
}
.lunbo image{
  width: 100%;
  height: 100%;
}
/* 分类 */
.fenlei{
  width: 100%;
  box-sizing: border-box;
  background: #fff;
  overflow: hidden;
  padding-bottom: 30rpx;
}
.fenlei-list{
  float: left;
  width: 25%;
  text-align: center;
  box-sizing: border-box;
  margin-top: 30rpx;
}
.fenlei-list image{
  width: 85rpx;
  height: 85rpx;
  border-radius: 50%;
}
.fenlei-list view{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-family: "微软雅黑";
  color: #242424;
  font-size: 28rpx;
}
/* 头条 */
.toutiao{
  width: 100%;
  padding: 20rpx;
  height: 100rpx;
  background: #fff;
  box-sizing: border-box;
  overflow: hidden;
  margin: 2% 0;
}
.toutiao view{
  float: left;
}
.toutiao-tu{
  width: 70rpx;
  height: 65rpx;
  margin-top: -2rpx;
}
.toutiao-tu image{
  width: 100%;
  height: 100%;
}
.toutiao-xian{
  height: 30rpx;
  border-left: 1px solid #DEDEDE;
  margin: 2.5%;
}
.toutiao-title{
  width: 600rpx;
  height: 65rpx;
  line-height: 65rpx;
  font-family: "微软雅黑";
  color: #242424;
  font-size: 28rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
/* 图文 */
.tuwen{
  width: 100%;
  box-sizing: border-box;
  background: #fff;
  overflow: hidden;
}
.tuwen-left{
  width: 50%;
  padding: 4%;
  box-sizing: border-box;
  float: left;
  border-right: 1px solid #E9E9E9;
}
.tuwen-left .tuwen-tu{
  width: 100%;
  height: 150rpx;
  border-radius: 50rpx;
}
.tuwen-left .tuwen-tu image{
  width: 100%;
  height: 100%;
  border-radius: 50rpx;
}
.tuwen-left .tuwen-wen{
  width: 100%;
  height: 80rpx;
  text-align: center;
  overflow: hidden;
  font-family: "微软雅黑";
  font-size: 28rpx;
  color: #000;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
}
.tuwen-right{
  width: 50%;
  float: left;
  box-sizing: border-box;
}
.tuwen-list{
  width: 100%;
  padding: 6%;
  box-sizing: border-box;
  overflow: hidden;
}
.tuwen-list:first-child{
  border-bottom: 1px solid #F4F4F4;
}
.tuwen-img{
  float: left;
  width: 100rpx;
  height: 100rpx;
  border-radius: 50%;
}
.tuwen-img image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}
.tuwen-title{
  float: left;
  width: 200rpx;
  height: 80rpx;
  text-align: center;
  overflow: hidden;
  font-family: "微软雅黑";
  font-size: 28rpx;
  color: #000;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2; 
  margin-top: 8rpx;
  margin-left: 20rpx;
}
/* 特色服务 */
.fuwu{
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  background: #fff;
  margin-top: 2%;
}
.fuwu-title{
  width: 100%;
  text-align: center;
  font-family: "微软雅黑";
  font-size: 30rpx;
  color: #000;
  font-weight: bold;
}
.fuwu-list{
  display: inline-block;
  width: 230rpx;
  margin-right: 10rpx;
  margin-top: 20rpx;
}
.fuwu-list:nth-child(3),.fuwu-list:nth-child(6),.fuwu-list:nth-child(9){
  margin-right: 0;
}
.fuwu-list image{
  width: 100%;
  height: 188rpx;
}
.fuwu-list view{
  width: 100%;
  text-align: center;
  font-family: "微软雅黑";
  font-size: 28rpx;
  color: #000;
}
.fuwu-list view:last-child{
  font-size: 26rpx;
  color: #A4A4A4;
}
/* 长图 */
.ct{
  width: 100%;
  height: 220rpx;
  margin: 2% 0;
}
.ct image{
  width: 100%;
  height: 100%;
}
/* 猜你喜欢 */
.love{
  width: 100%;
  background: #fff;
  padding: 20rpx;
  box-sizing: border-box;
}
.love-list{
  width: 100%;
  padding: 20rpx 0;
  box-sizing: border-box;
  overflow: hidden;
  border-bottom: 1px solid #eee;
}
.love-list:nth-child(2){
  border-top: 1px solid #eee;
  margin-top: 20rpx;
}
.love-list image{
  width: 120rpx;
  height: 120rpx;
  float: left;
}
.love-txt{
  width: 590rpx;
  float: left;
  padding: 5rpx 0 5rpx 20rpx;
  box-sizing: border-box;
}
.love-txt view{
  width: 100%;
  font-family: "微软雅黑";
  font-size: 28rpx;
  color: #000;
}
.love-txt view.love-txt1{
  height: 40rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}
.love-txt view.love-txt2{
  height: 70rpx;
  font-size: 26rpx;
  color: #A4A4A4;
  overflow: hidden;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  margin-top: 2rpx;
}
/* 入住名额 */
.qiang{
  width: 100%;
  padding: 20rpx;
  box-sizing: border-box;
  background: #fff;
  margin-top: 20rpx;
}
.qiang-form{
  width: 100%;
  margin-top: 20rpx;
  box-sizing: border-box;
}
.qiang-form view{
  width: 100%;
  overflow: hidden;
  margin-bottom: 20rpx;
}
.qiang-form view input{
  width: 100%;
  height: 80rpx;
  padding: 0 20rpx;
  box-sizing: border-box;
  font-family: "微软雅黑";
  color: #000;
  font-size: 26rpx;
  border: 1px solid #eee;
}
.qiang-form view radio-group label{
  font-family: "微软雅黑";
  color: #000;
  font-size: 26rpx;
}
.qiang-form view radio-group label:first-child{
  float: left;
}
.qiang-form view radio-group label:last-child{
  float: right;
}
.qiang-form form button{
  width: 100%;
  border-radius: 0;
  background: #F79200;
  font-family: "微软雅黑";
  font-size: 30rpx;
  color: #fff;
}
button::after{
  border: none;
}
.qiang-di{
  width: 100%;
  margin-top: 20rpx;
  text-align: center;
  font-family: "微软雅黑";
  font-size: 28rpx;
  color: #606060;
}
.qiang-di text{
  color: #EDA983;
}
/* 版权 */
.banquan{
  width: 100%;
  box-sizing: border-box;
  text-align: center;
  padding: 20rpx 0;
}
.banquan image{
  width: 55rpx;
  height: 55rpx;
  margin-right: 10rpx;
}
.banquan-conp{
  display: inline-block;
  height: 60rpx;
}
.banquan-conp text{
  display: block;
  font-family: "微软雅黑";
  font-size: 22rpx;
  color: #8A8A8A;
}
/* 底部导航 */
.nav{
  width: 100%;
  height: 95rpx;
  box-sizing: border-box;
  position: fixed;
  bottom: 0;left: 0;
}
.nav button{
  display: inline-block;
  width: 33.33%;
  height: 95rpx;
  line-height: 67rpx;
  text-align: center;
  padding: 20rpx;
  box-sizing: border-box;
  border-radius: 0;
}
.nav button image{
  width: 45rpx;
  height: 45rpx;
  margin-right: 10rpx;
}
.nav button:nth-child(1){
  background: #C9355B;
}
.nav button:nth-child(2){
  background: #E49015;
}
.nav button:nth-child(3){
  background: #59325B;
}
.nav button text{
  font-family: "微软雅黑";
  font-size: 30rpx;
  color: #fff;
  font-weight: bold;
  position: relative;
  top: -11rpx;
}
/* 隐藏导航 */
.hn{
  width: 500rpx;
  height: 520rpx;
  border-radius: 20rpx;
  box-sizing: border-box;
  background: rgba(0, 0, 0, 0.6);
  position: fixed;
  bottom: 110rpx;
  right: 20rpx;
}
.hn-fenlei{
  width: 100%;
  box-sizing: border-box;
  overflow: hidden;
  padding-bottom: 30rpx;
}
.hn-fenlei-list{
  float: left;
  width: 33.33%;
  text-align: center;
  box-sizing: border-box;
  margin-top: 30rpx;
}
.hn-fenlei-list image{
  width: 85rpx;
  height: 85rpx;
  border-radius: 50%;
}
.hn-fenlei-list view{
  width: 100%;
  height: 40rpx;
  line-height: 40rpx;
  text-align: center;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  font-family: "微软雅黑";
  color: #fff;
  font-size: 28rpx;
}
view.show{
  display: block;
}
view.hide{
  display: none;
}
.zhuan{
  width: 60rpx;
  height: 60rpx;
  border-radius: 50%;
  position: fixed;
  bottom: 200rpx;
  right: 45rpx;
  z-index: 2000;
}
.zhuan image{
  width: 100%;
  height: 100%;
  border-radius: 50%;
}